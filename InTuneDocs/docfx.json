{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "Understand/**",
          "DeployUse/**",
          "PlanDesign/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "src": "DeployUse",
        "dest": "deploy-use"
      },
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "src": "Understand",
        "dest": "understand-explore"
      },
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "src": "PlanDesign",
        "dest": "plan-design"
      }
    ],
    "resource": [
        {
          "files": [
          "**/images/**",
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*.html",
          "**/*.css"
          ],
        "exclude": [
          "**/obj/**",
          "_themes/**",
          "Understand/**",
          "DeployUse/**",
          "PlanDesign/**"
        ]
      }
    ],
    "dest": "InTuneDocs",
    "template": "docs.html",
    "globalMetadata": {
      "layout": "Conceptual",
      "breadcrumb_path": "/enterprise-mobility/toc.json",
      "product_feedback_displaytext" : "Intune Feedback",
      "product_feedback_url": "https://microsoftintune.uservoice.com/"
    }
  }
}
